{
  "name": "simple-graphql",
  "version": "3.1.1",
  "description": "The simple way to generates GraphQL schemas and Sequelize models from your models definition.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "format": "standardx --fix src/**/*.ts demo/**/*.ts __tests__/**/*.ts",
    "test": "jest",
    "demo": "babel-node demo/index.ts --extensions '.ts'",
    "build": "rm -rf dist && tsc",
    "buildAndPublish": "npm run format && npm run build && npm publish"
  },
  "author": "logerzhu",
  "repository": {
    "type": "git",
    "url": "git@github.com:logerzhu/simple-graphql.git"
  },
  "bugs": {
    "url": "https://github.com/logerzhu/simple-graphql/issues"
  },
  "homepage": "https://github.com/logerzhu/simple-graphql",
  "license": "MIT",
  "keywords": [
    "GraphQL",
    "ORM",
    "Relay",
    "Sequelize",
    "MySQL",
    "PostgreSQL",
    "SQLite",
    "MSSQL"
  ],
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "@types/bluebird": "^3.5.29",
    "@types/jest": "^25.1.5",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.8",
    "@types/validator": "^12.0.1",
    "continuation-local-storage": "^3.2.1",
    "dataloader": "^2.0.0",
    "lodash": "^4.17.10",
    "lru_map": "^0.3.3",
    "md5": "^2.3.0",
    "moment": "^2.22.2",
    "sequelize": "5.21.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.9.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.9.0",
    "express": "^4.16.4",
    "express-graphql": "^0.9.0",
    "graphql": "^14.1.1",
    "graphql-relay": "^0.6.0",
    "jest": "^25.2.7",
    "mysql2": "^1.7.0",
    "pre-commit": "^1.2.2",
    "sqlite3": "^4.2.0",
    "standardx": "^5.0.0",
    "ts-jest": "^25.3.0",
    "typescript": "^3.8.3"
  },
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "off"
    }
  },
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ],
    "ignore": [
      "/dist/",
      "/docs/"
    ]
  },
  "pre-commit": [
    "format"
  ]
}
